Last login: Sun Nov  2 16:13:17 2025 from 113.189.109.248
root@vmi2881987:~# # 1) Проверка статуса и логов API
systemctl restart connexa-api || true
systemctl status connexa-api --no-pager -l || true
journalctl -u connexa-api -n 200 --no-pager || true

# 2) Если видите ModuleNotFoundError на api.setup или api.export (или любые импорты api.*),
# временно отключим их, чтобы API запустился немедленно; затем перезапустим сервис.
sed -i 's/^from api\.setup/# &/; s/^from api\.export/# &/; s/^app\.include_router(setup_router)/# &/; s/^app\.include_router(export_router)/# &/' /opt/Connexa-Free-Proxy/api/server.py
systemctl restart connexa-api

# 3) Тест локально
curl -fsS http://127.0.0.1:8080/healthz && echo "API OK" || echo "API still down"

# 4) Откроем порт 8080 в UFW (если UFW активен)
ufw allow 8080/tcp || true
